<div class="banner" style="background-image: linear-gradient(-225deg, rgba(0,101,168,0.2) 0%, rgba(0,36,61,0.2) 50%), url('http://res.cloudinary.com/dujnmeiiu/image/upload/v1468499876/photostudio_yzfecj.jpg');">
  <div class="banner-content">
    <h1>Welcome to Shootailor</h1>
    <p>Live the moment, we capture it for you</p>
    <!-- <a class="btn btn-primary btn-lg">GET IT</a> -->
    <% if user_signed_in? %>
      <%= link_to "Find a photographer", users_path, class: "btn btn-primary btn-lg" %>
    <% else %>
      <%= link_to "Sign In", new_user_session_path, class: "btn btn-primary btn-lg" %>
    <% end %>
  </div>
</div>

