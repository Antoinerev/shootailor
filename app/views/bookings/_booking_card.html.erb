

<% @bookings.each do |booking| %>
  <%= link_to user_path(booking.id) do %>
    <div class="row">
      <div class="col-xs-12 col-sm-4 col-sm-offset-4">
        <% photog = User.find(booking.photog_id) %>
        <%= "#{booking.created_at} - #{booking.shooting_location} - #{photog.last_name}" %>
      </div>
    </div>
  <% end %>
<% end %>
